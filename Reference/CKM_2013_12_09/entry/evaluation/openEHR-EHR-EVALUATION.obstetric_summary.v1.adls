				text = <"Gravidity (G)">
				description = <"Number of times a woman has been pregnant, including the current pregnancy, if appropriate.">
			>
			["id4"] = <
				text = <"Parity">
				description = <"Number of times a woman has given birth after 20 weeks and 0 days gestation.">
			>
			["id5"] = <
